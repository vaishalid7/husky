{
  "name": "code-quality",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "fix": "run-script-os",
    "fix:win32": "prettier-eslint --write %INIT_CWD%/src/**/*.js",
    "fix:darwin:linux": "prettier-eslint --write $PWD/src/**/*.js",
    "check-types": "tsc -p tsconfig.json --noEmit",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.13.8",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "prettier-eslint-cli": "^5.0.1",
    "run-script-os": "^1.1.5",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "react": "^17.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm-run-all --parallel check-types lint test"
    }
  },
  "lint-staged": {
    "*.{js, json, jsx}": "eslint --cache --fix",
    "*.{ts,tsx}": [
      "eslint --cache --fix",
      "tsc --noEmit --jsx react --skipLibCheck --esModuleInterop"
    ]
  }
}
